{
  "name": "wingrr",
  "version": "0.0.1",
  "description": "Wing Rosetta Runtime",
  "scripts": {
    "install": "npm-run-all cmake",
    "cmake": "npm-run-all cmake:*",
    "cmake:wingrr": "cmake-js -m -d",
    "clean": "npm-run-all clean:*",
    "clean:wingrr": "rm -rf build",
    "rebuild": "npm-run-all clean cmake",
    "test": "LD_LIBRARY_PATH=$(pwd)/build/Release npm-run-all *:test",
    "cargo:test": "cargo test --manifest-path src/crate/Cargo.toml -- --test-threads=1",
    "npm:test": "npm-run-all test:*",
    "test:js": "./build/wingrr tests/hello.js ./",
    "test:ts": "./build/wingrr tests/hello.ts ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monadahq/winglang.git"
  },
  "author": "Wing Developers",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/monadahq/winglang/issues"
  },
  "homepage": "https://github.com/monadahq/winglang#readme",
  "dependencies": {
    "cmake-js": "^6.3.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.8.2"
  }
}
