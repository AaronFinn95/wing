{
  "name": "wingrr",
  "version": "0.0.1",
  "description": "Wing Rosetta Runtime",
  "scripts": {
    "install": "npm-run-all cmake",
    "cmake": "npm-run-all cmake:*",
    "cmake:nodejs": "cmake-js -m -d vendor/node",
    "cmake:wingrr": "cmake-js -m -d",
    "clean": "npm-run-all clean:*",
    "clean:wingrr": "rm -rf build",
    "clean:nodejs": "rm -rf vendor/node/build vendor/node/bin",
    "rebuild": "npm-run-all clean cmake",
    "test": "npm-run-all test:*",
    "test:js": "./build/wingrr tests/hello.js ./",
    "test:ts": "./build/wingrr tests/hello.ts ./",
    "test:py": "./build/wingrr tests/hello.py ./",
    "test:rb": "./build/wingrr tests/hello.rb ./",
    "test:cs": "./build/wingrr tests/hello.cs ./",
    "test:go": "./build/wingrr tests/hello.go ./",
    "test:lua": "./build/wingrr tests/hello.lua ./",
    "test:java": "./build/wingrr tests/hello.java ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monadahq/winglang.git"
  },
  "author": "Wing Developers",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/monadahq/winglang/issues"
  },
  "homepage": "https://github.com/monadahq/winglang#readme",
  "dependencies": {
    "cmake-js": "^6.3.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.8.2"
  }
}
