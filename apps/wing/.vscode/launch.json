{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Wing CLI",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/index-pkg.ts",
      "runtimeArgs": [
        "--loader",
        "tsx",
        "--require",
        "tsx/suppress-warnings",
        "--experimental-wasi-unstable-preview1",
        "--no-warnings"
      ]
    }
  ]
}
