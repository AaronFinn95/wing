{
  "name": "@monadahq/wing",
  "version": "0.0.0",
  "description": "Wing Programming Language",
  "engines": {
    "node": ">=18"
  },
  "main": "src/index-node.js",
  "bin": "src/index-node.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monadahq/winglang.git"
  },
  "scripts": {
    "copy-wingc-debug": "cp ../../target/wasm32-wasi/debug/wingc.wasm ./",
    "build": "npm-run-all build:*",
    "build:clean": "rimraf wingsdk.tgz bin .wing",
    "build:pack": "npm-run-all build:pack:*",
    "build:pack:wingsdk": "cd ../../libs/wingsdk && npm pack && mv monadahq-wingsdk-*.tgz ../../apps/wing/wingsdk.tgz",
    "build:pack:install": "rimraf temp && mkdir temp && npm install --prefix=temp wingsdk.tgz",
    "build:pack:cleanup": "rimraf 'temp/*/*cdktf*/**/*/.jsii' rimraf 'temp/**/*/*.md' 'temp/**/*/*.d.ts' 'temp/package*.json' wingsdk.tgz",
    "build:pack:tarball": "tar -czf wingsdk.tgz -C temp node_modules && rimraf temp",
    "build:pkg": "pkg --compress GZip --options experimental-wasi-unstable-preview1,no-warnings -c package.json src/index-pkg.js"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "debug": "^4.3.4",
    "decompress": "^4.2.1",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0",
    "rimraf": "^3.0.2"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-darwin-x64",
      "node18-win32-x64"
    ],
    "assets": [
      "wingc.wasm",
      "wingsdk.tgz"
    ],
    "outputPath": "bin"
  }
}
