{
  "name": "wing",
  "implicitDependencies": [
    "wingc",
    "wingsdk"
  ],
  "targets": {
    "copy": {
      "executor": "nx:run-commands",
      "inputs": [
        "wingc-wasm"
      ],
      "dependsOn": [
        "^build-wasm"
      ],
      "outputs": [
        "{projectRoot}/wingc.wasm"
      ],
      "options": {
        "command": "cp ../../target/wasm32-wasi/debug/wingc.wasm ./",
        "cwd": "apps/wing",
        "color": true
      },
      "configurations": {
        "release": {
          "command": "cp ../../target/wasm32-wasi/release/wingc.wasm ./"
        }
      }
    },
    "build": {
      "executor": "nx:run-script",
      "dependsOn": [
        "^build",
        "copy"
      ],
      "options": {
        "script": "build",
        "color": true
      }
    }
  }
}
