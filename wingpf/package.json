{
  "name": "wingpf",
  "version": "0.0.1",
  "description": "Wing Programming Language Preflight Runtime",
  "scripts": {
    "install": "npm-run-all patch cmake",
    "patch": "node ./src/patch-pyodide.js",
    "cmake": "npm-run-all cmake:*",
    "cmake:pyodide": "webpack",
    "cmake:node": "cmake-js -m -d vendor/node",
    "cmake:wingpf": "cmake-js -m -d",
    "clean": "npm-run-all clean:*",
    "clean:wingpf": "rm -rf build",
    "clean:node": "rm -rf vendor/node/build vendor/node/bin",
    "clean:pyodide": "rm -rf dist",
    "rebuild": "npm-run-all clean cmake"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monadahq/winglang.git"
  },
  "keywords": [],
  "author": "Wing Developers",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/monadahq/winglang/issues"
  },
  "homepage": "https://github.com/monadahq/winglang#readme",
  "dependencies": {
    "cmake-js": "^6.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "node-fetch": "^3.2.6",
    "npm-run-all": "^4.1.5",
    "pyodide": "^0.20.1-alpha.2",
    "string-replace-loader": "^3.1.0",
    "ts-node": "^10.8.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
